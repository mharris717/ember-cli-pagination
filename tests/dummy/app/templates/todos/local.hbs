<table class="table">
  <tr>
    <th>Name</th>
    <th>Completed</th>
  </tr>

  {{#each pagedContent as |content|}}
    <tr class="todo">
      <td>{{content.name}}</td>
      <td>
        <Input @type="checkbox" @checked={{content.completed}} />
      </td>
    </tr>
  {{/each}}
</table>

<PageNumbers @content={{pagedContent}} />

<button {{action "save"}}>Save</button>

<div class="numRemoteCalls">
  {{pagedContent.numRemoteCalls}}
</div>

{{outlet}}

{
  "name": "ember-cli-pagination",
  "version": "3.1.5",
  "private": false,
  "description": "Addon for Ember CLI to do simple pagination. Compatible with the kaminari API in Rails",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/mharris717/ember-cli-pagination",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
